{
  "name": "autopilot",
  "displayName": "autopilot",
  "description": "Build on top of powerful openai API to give you edge over development.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "publisher": "bhushanlaware",
  "browser": "./dist/extension.js",
  "contributes": {
    "menus": {
      "view/title": [
        {
          "command": "autopilot.chatHistory",
          "group": "autopilot.menu.basic",
          "when": "view == autopilot.chat"
        },
        {
          "command": "autopilot.clearHistory",
          "group": "autopilot.menu.basic",
          "when": "view == autopilot.chat"
        }
      ]
    },
    "commands": [
      {
        "command": "autopilot.chatHistory",
        "title": "Switch Chat"
      },
      {
        "command": "autopilot.startNew",
        "title": "Start New Chat"
      },
      {
        "command": "autopilot.reset",
        "title": "Reset Autopilot"
      },
      {
        "command": "autopilot.clearHistory",
        "title": "Delete All Chats"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "autopilot-chat-view",
          "title": "Autopilot",
          "icon": "$(hubot)"
        }
      ]
    },
    "views": {
      "autopilot-chat-view": [
        {
          "type": "webview",
          "id": "autopilot.chat",
          "name": "AI Chat"
        }
      ]
    },
    "configuration": {
      "title": "Autopilot",
      "properties": {
        "autopilot.chatModel": {
          "type": "string",
          "name": "Chat Model",
          "enum": [
            "gpt-3.5-turbo",
            "gpt-4"
          ],
          "default": "gpt-3.5-turbo",
          "description": "Choose the GPT model to use for Chat."
        },
        "autopilot.chatTemperature": {
          "type": "number",
          "default": 0.5,
          "minimum": 0,
          "maximum": 1,
          "description": "Set the temperature for the GPT model."
        },
        "autopilot.completionModel": {
          "type": "string",
          "enum": [
            "text-davinci-003",
            "text-davinci-002",
            "text-curie-001",
            "text-babbage-001",
            "text-ada-001",
            "davinci",
            "curie",
            "babbage",
            "ada"
          ],
          "default": "text-davinci-002",
          "description": "Choose the GPT model to use for Chat."
        },
        "autopilot.completionTemperature": {
          "type": "number",
          "default": 0.5,
          "minimum": 0,
          "maximum": 1,
          "description": "Set the temperature for the GPT model."
        },
        "autopilot.completionMaxTokens": {
          "type": "number",
          "default": 200,
          "minimum": 10,
          "maximum": 8000,
          "description": "Set the temperature for the completion."
        },
        "autopilot.numberOfCompletions": {
          "type": "number",
          "default": 1,
          "enum": [
            1,
            2,
            3,
            4,
            5
          ]
        },
        "autopilot.openaiAPIKey": {
          "type": "string",
          "default": "",
          "description": "Open API Key"
        },
        "autopilot.chatId": {
          "type": "string",
          "default": "",
          "description": "Current chat id"
        },
        "autopilot.enableFileIndexing": {
          "type": "boolean",
          "default": false,
          "description": "Enable Sementic search for getting better context our project, Please keep in mind this might increase billing."
        }
      }
    }
  },
  "scripts": {
    "test": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. --extensionTestsPath=dist/web/test/suite/index.js",
    "pretest": "yarn run compile-web",
    "vscode:prepublish": "yarn run package-web",
    "compile-web": "webpack --config extension.webpack.config.js && yarn build-react",
    "watch-web": "webpack --watch --config extension.webpack.config.js",
    "compile": "gulp compile-extension:autopilot",
    "lint": "eslint src --ext ts",
    "run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. .",
    "build-react": "webpack build --config ./webpack-react.config.js",
    "watch-react": "webpack watch --config ./webpack-react.config.js",
    "watch": "yarn watch-react & yarn watch-web",
    "extension": "vsce package",
    "package-web": "yarn build-react && webpack --mode production --devtool hidden-source-map --config extension.webpack.config.js"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@types/lodash": "^4.14.195",
    "@types/mocha": "^10.0.1",
    "@types/react": "^18.0.2",
    "@types/react-dom": "^18.0.0",
    "@types/react-syntax-highlighter": "^15.5.6",
    "@types/semver": "^7.3.9",
    "@types/vscode": "^1.70.0",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vscode/test-web": "^0.0.34",
    "assert": "^2.0.0",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.33.0",
    "mini-css-extract-plugin": "^2.4.5",
    "mocha": "^10.1.0",
    "process": "^0.11.10",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {
    "copy-to-clipboard": "^3.3.3",
    "eventsource-parser": "^0.1.0",
    "lodash": "^4.17.21",
    "openai": "^3.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-markdown": "^8.0.5",
    "react-syntax-highlighter": "^15.5.0",
    "rehype-raw": "^6.1.1",
    "remark-gfm": "^3.0.1"
  }
}